<h2>Quizzes for "<%= @assignment.name %>"</h2>

<% if @assignment.quiz_allowed(@reviewee_topic_id) or @assignment.finished?(@reviewee_topic_id) %>
  <%= render :partial => 'responses', :locals => {:mappings => @quiz_mappings} %>
  <%= render :partial => 'set_dynamic_quiz', :locals => {:assignment => @assignment, :participant => @participant} %>
<% else %>
  <p><em>This assignment does not support quizzes</em></p>
<% end %>

<br><br>
<a href="javascript:window.history.back()">Back</a>
